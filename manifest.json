{
    "manifest_version": 3,
    "name": "URL Blocker",
    "version": "1.0",
    "permissions": ["storage", "webRequest", "webRequestBlocking"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "dist/background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_title": "Block URLs"
    }
  }
  
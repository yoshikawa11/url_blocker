{
  "manifest_version": 3,
  "name": "URL Blocker",
  "version": "1.0",
  "permissions": [
    "declarativeNetRequest",
    "declarativeNetRequestWithHostAccess",
    "storage"
  ],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Block URLs"
  },
  "background": {
    "service_worker": "dist/background.js"
  }
}


  